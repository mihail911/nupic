<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
  ProjectType="Visual C++"
  Version="${VisualStudioVersion}"
  Name="$ProjectName"
  ProjectGUID="%(GUID)s"
  RootNamespace="$ProjectName"
  Keyword="Win32Proj"
  >
  <Platforms>
    <Platform
      Name="Win32"
    />
  </Platforms>
  <ToolFiles>
  </ToolFiles>
  <Configurations>
    <Configuration
      Name="Debug|Win32"
      OutputDirectory="${Win32InstallDir}\bin"
      IntermediateDirectory="${Win32BuildDir}\apps\${ProjectName}\$$(ConfigurationName)"
      ConfigurationType="1"
      CharacterSet="1"
      WholeProgramOptimization="0"
      >
      <Tool
        Name="VCCLCompilerTool"
        Optimization="0"        
        AdditionalIncludeDirectories="${RelativeRootDir};${RelativeRootDir}/external/win32/include;${RelativeRootDir}/external/common/include"
        PreprocessorDefinitions="NOMINMAX;NTA_INTERNAL;NTA_PLATFORM_win32;NUPIC2;NTA_INSTRUMENT_ON;_CRT_SECURE_NO_DEPRECATE;_SCL_SECURE_NO_WARNINGS;APR_DECLARE_STATIC;APU_DECLARE_STATIC;XML_STATIC;PCRE_STATIC;PCREPOSIX_STATIC;WIN32;_HAS_ITERATOR_DEBUGGING=0;_SECURE_SCL=0;NTA_TEST_WINDOWS;NTA_PYTHON_VERSION_NUM=${PythonVersionNum}"
        RuntimeLibrary="0"
        UsePrecompiledHeader="0"
        WarningLevel="3"
        DebugInformationFormat="3"
      />
      <Tool
        Name="VCLinkerTool"
        AdditionalDependencies="apr-1.MT.lib aprutil-1.MT.lib apriconv-1.MT.lib zlib.MT.lib Mswsock.lib ws2_32.lib pcre.MT.lib libeay32.MT.lib Rpcrt4.lib Secur32.lib Crypt32.lib Mpr.lib Ntdsapi.lib Netapi32.lib Activeds.lib ADSIid.lib psapi.lib yamlcpp.MT.lib"
        LinkIncremental="1"
        GenerateManifest="false"
        AdditionalLibraryDirectories="${RelativeRootDir}/external/win32/lib;${Win32PythonDir}\libs"
        IgnoreAllDefaultLibraries="false"
        IgnoreDefaultLibraryNames="libcmtd.lib;msvcrtd.lib;atls.lib"
        GenerateDebugInformation="true"
        SubSystem="1"
        OptimizeReferences="2"
        EnableCOMDATFolding="2"
        TargetMachine="1"
      />
    </Configuration>
    
    <Configuration
      Name="Release|Win32"
      OutputDirectory="${Win32InstallDir}\bin"
      IntermediateDirectory="${Win32BuildDir}\apps\${ProjectName}\$$(ConfigurationName)"
      ConfigurationType="1"
      CharacterSet="1"
      WholeProgramOptimization="1"
      >
      <Tool
        Name="VCCLCompilerTool"
        Optimization="2"        
				EnableIntrinsicFunctions="true"
        AdditionalIncludeDirectories="${RelativeRootDir};${RelativeRootDir}/external/win32/include;${RelativeRootDir}/external/common/include"
        PreprocessorDefinitions="NOMINMAX;NTA_INTERNAL;NTA_PLATFORM_win32;NUPIC2;NTA_INSTRUMENT_ON;_CRT_SECURE_NO_DEPRECATE;_SCL_SECURE_NO_WARNINGS;APR_DECLARE_STATIC;APU_DECLARE_STATIC;XML_STATIC;PCRE_STATIC;PCREPOSIX_STATIC;WIN32;_HAS_ITERATOR_DEBUGGING=0;_SECURE_SCL=0;NTA_TEST_WINDOWS;NTA_PYTHON_VERSION_NUM=${PythonVersionNum}"
        RuntimeLibrary="0"
        UsePrecompiledHeader="0"
        WarningLevel="3"
        DebugInformationFormat="3"
      />
      <Tool
        Name="VCLinkerTool"
        AdditionalDependencies="apr-1.MT.lib aprutil-1.MT.lib apriconv-1.MT.lib zlib.MT.lib Mswsock.lib ws2_32.lib pcre.MT.lib libeay32.MT.lib Rpcrt4.lib Secur32.lib Crypt32.lib Mpr.lib Ntdsapi.lib Netapi32.lib Activeds.lib ADSIid.lib psapi.lib yamlcpp.MT.lib"
        LinkIncremental="1"
        GenerateManifest="false"
        AdditionalLibraryDirectories="${RelativeRootDir}/external/win32/lib;${Win32PythonDir}\libs"
        IgnoreAllDefaultLibraries="false"
        IgnoreDefaultLibraryNames="libcmtd.lib;msvcrtd.lib;atls.lib"
        GenerateDebugInformation="true"
        SubSystem="1"
        OptimizeReferences="2"
        EnableCOMDATFolding="2"
        TargetMachine="1"
      />
    </Configuration>
  </Configurations>
  <References>
$References
  </References>
  <Files>
    <Filter
      Name="Source Files"
      Filter="cpp;c;cc;cxx;def;odl;idl;hpj;bat;asm;asmx"
      UniqueIdentifier="{00000000-0000-0000-0000-00000000}"
      >
$SourceFiles

    </Filter>
    <Filter
      Name="Header Files"
      Filter="h;hpp;hxx;hm;inl;inc;xsd"
      UniqueIdentifier="{00000000-0000-0000-0000-00000000}"
      >
$HeaderFiles
    </Filter>
  </Files>
  <Globals>
  </Globals>
</VisualStudioProject>
