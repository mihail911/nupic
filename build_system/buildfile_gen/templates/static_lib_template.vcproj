      OutputDirectory="${Win32BuildDir}\output\$$(ConfigurationName)"
      IntermediateDirectory="${Win32BuildDir}\nta\${ProjectName}\$$(ConfigurationName)"
      ConfigurationType="4"
      CharacterSet="1"
