<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="InstallPython"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="InstallPython"
			DisplayName="InstallPython"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(NTAX_INSTALL_DIR)/lib/python$(NTAX_PYTHON_VERSION)/site-packages/nupic/bindings/$(InputName).py&quot;"
			Outputs="$(NTAX_INSTALL_DIR)/lib/python$(NTAX_PYTHON_VERSION)/site-packages/nupic/bindings/$(InputName).py"
			FileExtensions="*.py"
			ExecutionDescription="Installing generated Python file &quot;$(InputPath)&quot;..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
