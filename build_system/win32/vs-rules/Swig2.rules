<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Swig"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Swig"
			DisplayName="Swig"
			CommandLine="&quot;$(SolutionDir)external\win32\swigwin-1.3.36\swig.exe&quot; -c++ -python -features autodoc=0,directors=0 -noproxyimport -keyword -modern -modernargs -noproxydel -castmode -fvirtual -fastunpack -nofastproxy -fastquery -outputtuple -w511 -w302 -w362 -w312 -I&quot;$(SolutionDir).&quot; -I&quot;$(SolutionDir)nta\python\bindings\numpy&quot; -DNTA_PLATFORM_win32 -DWIN32 -DNTA_PYTHON_SUPPORT -DNUPIC2 -o &quot;win_$(InputName).h&quot; &quot;$(InputPath)&quot;"
			Outputs="win_$(InputName).h"
			FileExtensions="*.i"
			ExecutionDescription="Swigging &quot;$(InputPath)&quot;"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
